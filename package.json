{
  "name": "prettier-config-boilerplate",
  "displayName": "Prettier Config Boilerplate",
  "description": "A template config for prettier config files using snippets. Available in JSON, JS, YAML and TOML files.",
  "version": "1.0.2",
  "publisher": "MartinValdez",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/martinval11/vsc-prettier-config-boilerplate"
  },
  "keywords": [
    "prettier",
    "prettier snippets",
    "snippet",
    "snippets",
    "boilerplate"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martinval11/vsc-prettier-config-boilerplate/issues"
  },
  "homepage": "https://github.com/martinval11/vsc-prettier-config-boilerplate",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "json",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets-js.code-snippets"
      },
      {
        "language": "yaml",
        "path": "./snippets/snippets-yaml.code-snippets"
      },
      {
        "language": "toml",
        "path": "./snippets/snippets-toml.code-snippets"
      }
    ]
  }
}
